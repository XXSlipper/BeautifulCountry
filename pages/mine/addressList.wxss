/* addressList.wxss */

.insertAddressBtn{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 600rpx;
  height: 80rpx;
  background: #84b637;
  border-radius: 10rpx;
  position: fixed;
  bottom: 30px;
  left: 75rpx;
}

.addressListTable{
  display: flex;
  flex-direction: column;
}

.addressCell{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100%;
  height: 100px;
}

.cellUpArea{
  width:700rpx;
  height:50px;
  display: flex;
  flex-direction: column;
}

.cellDownArea{
  width: 700rpx;
  height: 40px;
  display: flex;
  justify-content: space-between;
}